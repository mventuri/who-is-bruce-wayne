<script>
function ChangeImage(){
var image = document.getElementById("theTruth")
	if (image.src.match("C:\Users\mvent_000\Desktop\bruce.jpg")) {
        image.src = "C:\Users\mvent_000\Desktop\batman-v-superman-purported-new-photos-of-ben-affl_wd82.1920.jpg";
		
		}
	else {
	image.src = "C:\Users\mvent_000\Desktop\bruce.jpg";
	}
}
</script>
